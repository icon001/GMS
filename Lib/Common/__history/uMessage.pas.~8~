unit uMessage;

interface
var
  //에러처리 메시지
  G_stMSGNAMEERROR :string = '$NAME가(이) 올바르지 않습니다.';
  G_stMSGSELECTERROR :string = '$WORK 데이터를 선택 하세요.';
  G_stMSGWORKCOUNTINFO :string = '$COUNT의 $WORK를(을) 수행하시겠습니까?';
  G_stMSGEXISTNOTWORK : string = '$EXISTWORK가(의) 존재하여 $CURRENTWORK를(을) 수행 할 수 없습니다.';

implementation

end.
